<%= simple_form_for(@registration) do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :title %>
  <%= f.input :other %>
  <%= f.input :publication %>
  <%= f.input :type_of_media %>
  <%= f.input :address1 %>
  <%= f.input :address2 %>
  <%= f.input :city %>
  <%= f.input :state %>
  <%= f.input :province %>
  <%= f.input :postal_code %>
  <%= f.input :country %>
  <%= f.input :email %>
  <%= f.input :phone %>
  <%= f.input :extension %>
  <%= f.input :fax %>
  <%= f.input :website %>
  <%= f.input :media_outlet %>
  <%= f.input :comments %>
  <%= f.button :submit %>
<% end %>
